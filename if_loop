if __name__ == '__main__':
    n = int(input().strip())
    if n%2 != 0:
        print("Weird")
    else:
        print("Not Weird")
     
############################

if __name__ == '__main__':
    n = int(input().strip())
    if 1<=n<=100:
        if n%2:
            print("Weird")
        else:
            if (2<=n<=5) or (n>20):
                print("Not Weird")
            elif 6<=n<=20:
                print("Weird")
            else:
                print("Weird")
